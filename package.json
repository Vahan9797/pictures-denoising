{
  "name": "pictures-denoising",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "express": "^4.16.2",
    "express-react-views": "^0.10.4",
    "react": "^16.2.0",
    "react-bootstrap": "^0.32.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-scripts": "1.0.17",
    "redux": "^3.7.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/index.scss -o public/css/",
    "start": "nodemon --watch src --watch backend-implementation ./backend-implementation/index.js --exec babel-node --presets env,react",
    "build": "npm run build-css && ./node_modules/webpack/bin/webpack.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass-chokidar": "0.0.3",
    "nodemon": "^1.14.9",
    "webpack": "^3.10.0"
  }
}
